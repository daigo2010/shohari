<div class="well">
<p id="descriptionArea"><% if @exist %>it's result.<% else %>regist your self check and invite your friends.<% end %></p>
<iframe src="//www.facebook.com/plugins/like.php?href=http%3A%2F%2Fshohari.com%2F&amp;send=false&amp;layout=button_count&amp;width=100&amp;show_faces=true&amp;action=like&amp;colorscheme=light&amp;font&amp;height=21&amp;appId=208326842542197" scrolling="no" frameborder="0" style="border:none; overflow:hidden; width:100px; height:21px;" allowTransparency="true"></iframe>
<a href="https://twitter.com/share" class="twitter-share-button" data-url="http://shohari.com" data-text="他己分析サービス&quot;shohari&quot; - 他の人の評価から、あなたの知らない自分がわかるかも" data-via="shohari" data-lang="ja" data-hashtags="shohari">ツイート</a><script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="//platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");</script>
</div>

<% if @exist %>
<table align="center">
<tr>
<td>
<table class="table table-bordered window" id="openSelf">
    <tr><td style="text-align : center;" >
        <dl>
        <% @open_self.each_pair do |key, value| %>
            <li><%= value %></li>
        <% end %>
        </dl>
    </td></tr>
</table>
</td>
<td>
<table class="table table-bordered window" id="bindSelf">
    <tr><td style="text-align : center;">
        <dl>
        <% @bind_self.each_pair do |key, value| %>
            <li><%= value %></li>
        <% end %>
        </dl>
    </td></tr>
</table>
</td>
</tr>
<tr>
<td>
<table class="table table-bordered window" id="hiddenSelf">
    <tr><td style="text-align : center;">
        <dl>
        <% @hidden_self.each_pair do |key, value| %>
            <li><%= value %></li>
        <% end %>
        </dl>
    </td></tr>
</table>
</td>
<td>
<table class="table table-bordered window" id="unknownSelf">
    <tr><td style="text-align : center;">
        <dl>
        <% @unknown_self.each_pair do |key, value| %>
            <li><%= value %></li>
        <% end %>
        </dl>
    </td></tr>
</table>
</td>
</tr>
</table>
<% end %>
<%= link_to 'トップに戻る', '/', :class => 'btn' %>  
<script type="text/javascript">
(function() {
    var defaultDescription = "it's result";
    var openSelfDescription = "openSelf";
    var bindSelfDescription = "bindSelf";
    var hiddenSelfDescription = "hiddenSelf";
    var unknownSelfDescription = "unknownSelf";
    $("#openSelf").mouseover(function() {
        $("#descriptionArea").html(openSelfDescription);
    });
    $("#openSelf").mouseout(function() {
        $("#descriptionArea").html(defaultDescription);
    });
    $("#bindSelf").mouseover(function() {
        $("#descriptionArea").html(bindSelfDescription);
    });
    $("#bindSelf").mouseout(function() {
        $("#descriptionArea").html(defaultDescription);
    });
    $("#hiddenSelf").mouseover(function() {
        $("#descriptionArea").html(hiddenSelfDescription);
    });
    $("#hiddenSelf").mouseout(function() {
        $("#descriptionArea").html(defaultDescription);
    });
    $("#unknownSelf").mouseover(function() {
        $("#descriptionArea").html(unknownSelfDescription);
    });
    $("#unknownSelf").mouseout(function() {
        $("#descriptionArea").html(defaultDescription);
    });

})();
</script>
